<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:description_2="http://www.eclipse.org/sirius/tree/description/1.0.0" xmlns:description_3="http://www.eclipse.org/sirius/table/description/1.1.0" xmlns:properties="http://www.eclipse.org/sirius/properties/1.0.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" name="diagram" version="12.0.0.2017041100">
  <ownedViewpoints name="UserViewpoint" modelFileExtension="xmi">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="DiagramView" domainClass="cb::City" enablePopupBars="true">
      <defaultLayer name="Default">
        <nodeMappings name="StationNode" domainClass="cb::Station">
          <style xsi:type="style:BundledImageDescription" labelSize="12" showIcon="false" labelExpression="aql: self.name + ' - ' + self.availableBikes->size() + ' bikes available'" sizeComputationExpression="12" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
          </style>
          <conditionnalStyles predicateExpression="aql: self.availableBikes->size() &lt;= 0">
            <style xsi:type="style:BundledImageDescription" labelSize="12" showIcon="false" labelExpression="aql: self.name + '- No bikes available'" sizeComputationExpression="12" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
            </style>
          </conditionnalStyles>
          <conditionnalStyles predicateExpression="aql: self.availableBikes->size() == 1">
            <style xsi:type="style:BundledImageDescription" labelSize="12" showIcon="false" labelExpression="aql: self.name + ' - ' + self.availableBikes->size() + ' bike available'" sizeComputationExpression="12" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
            </style>
          </conditionnalStyles>
        </nodeMappings>
      </defaultLayer>
    </ownedRepresentations>
  </ownedViewpoints>
  <ownedViewpoints name="MechanicViewpoint" modelFileExtension="xmi">
    <ownedRepresentations xsi:type="description_2:TreeDescription" name="ServiceReportTree" showOnStartup="true" domainClass="cb::City">
      <subItemMappings name="BikeNoServiceTreeNode" domainClass="cb::Bike" semanticCandidatesExpression="service:getBikesWithNoService">
        <defaultStyle labelExpression="aql:self.name + ' (' + self.eContents() -> select(i|i.bike = self).bike -> count(self) + ')' + '  ' + self.lastServiceTime" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/bicycle-solid-red-30.png">
          <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
        </defaultStyle>
        <subItemMappings name="ServiceReportTreeNode" domainClass="cb::ServiceReport" semanticCandidatesExpression="feature:serviceReports">
          <defaultStyle labelExpression="aql: if self.mechanic.name->isEmpty() then 'Unknown'  else self.mechanic.name endif + ': ' + self.content" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/file-alt-solid-blue-30.png">
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </defaultStyle>
        </subItemMappings>
        <create name="Create Service Report">
          <firstModelOperation xsi:type="tool:CreateInstance" typeName="cb::ServiceReport" referenceName="serviceReports">
            <subModelOperations xsi:type="tool:ChangeContext" browseExpression="var:instance">
              <subModelOperations xsi:type="tool:SetValue" featureName="timestamp" valueExpression="service:getCurrentTime"/>
            </subModelOperations>
          </firstModelOperation>
          <variables name="root" documentation="The semantic element of the tree."/>
          <variables name="element" documentation="The currently edited element."/>
          <variables name="container" documentation="The semantic element corresponding to the view container."/>
        </create>
      </subItemMappings>
      <subItemMappings name="BikeNeedsServiceTreeNode" domainClass="cb::Bike" semanticCandidatesExpression="service:getBikesForService">
        <defaultStyle labelExpression="aql:self.name + ' (' + self.eContents() -> select(i|i.bike = self).bike -> count(self) + ')' + '  ' + self.lastServiceTime" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/bicycle-solid-red-30.png">
          <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
        </defaultStyle>
        <subItemMappings name="ServiceReportTreeNode" domainClass="cb::ServiceReport" semanticCandidatesExpression="feature:serviceReports">
          <defaultStyle labelExpression="aql: if self.mechanic.name->isEmpty() then 'Unknown'  else self.mechanic.name endif + ': ' + self.content" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/file-alt-solid-blue-30.png">
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </defaultStyle>
        </subItemMappings>
        <create name="Create Service Report">
          <firstModelOperation xsi:type="tool:CreateInstance" typeName="cb::ServiceReport" referenceName="serviceReports">
            <subModelOperations xsi:type="tool:ChangeContext" browseExpression="var:instance">
              <subModelOperations xsi:type="tool:SetValue" featureName="timestamp" valueExpression="service:getCurrentTime"/>
            </subModelOperations>
          </firstModelOperation>
          <variables name="root" documentation="The semantic element of the tree."/>
          <variables name="element" documentation="The currently edited element."/>
          <variables name="container" documentation="The semantic element corresponding to the view container."/>
        </create>
      </subItemMappings>
      <subItemMappings name="BikeTreeNode" domainClass="cb::Bike" semanticCandidatesExpression="service:getServicedBikes">
        <defaultStyle labelSize="9" labelExpression="aql:self.name + ' (' + self.eContents() -> select(i|i.bike = self).bike -> count(self) + ')' + '  ' + self.lastServiceTime" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/bicycle-solid-blue-30.png">
          <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
        </defaultStyle>
        <subItemMappings name="ServiceReportTreeNode" domainClass="cb::ServiceReport" semanticCandidatesExpression="aql:self.serviceReports->sortedBy(i | i.timestamp)">
          <defaultStyle labelSize="9" labelExpression="aql: if self.mechanic.name->isEmpty() then 'Unknown'  else self.mechanic.name endif + ': ' + self.content" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/file-alt-solid-blue-30.png">
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </defaultStyle>
        </subItemMappings>
        <create name="Create Service Report">
          <firstModelOperation xsi:type="tool:CreateInstance" typeName="cb::ServiceReport" referenceName="serviceReports">
            <subModelOperations xsi:type="tool:ChangeContext" browseExpression="var:instance">
              <subModelOperations xsi:type="tool:SetValue" featureName="timestamp" valueExpression="service:getCurrentTime"/>
            </subModelOperations>
          </firstModelOperation>
          <variables name="root" documentation="The semantic element of the tree."/>
          <variables name="element" documentation="The currently edited element."/>
          <variables name="container" documentation="The semantic element corresponding to the view container."/>
        </create>
      </subItemMappings>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="tdt4250.cb.diagram.Services"/>
  </ownedViewpoints>
  <ownedViewpoints name="AdminViewpoint" modelFileExtension="xmi">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="BikesAtStation" domainClass="cb::City" enablePopupBars="true">
      <defaultLayer name="Default">
        <nodeMappings name="StationNode" domainClass="cb::Station">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="11" showIcon="false" labelExpression="aql:self.name + '\n' + self.address" sizeComputationExpression="17" labelPosition="node" resizeKind="NSEW" workspacePath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/station-icon.png">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="BikeNode" doubleClickDescription="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikesAtStation']/@defaultLayer/@toolSections.2/@ownedTools[name='stationDoubleClick']" domainClass="cb::Bike">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql: '   ' + self.id + '  ' + self.name" sizeComputationExpression="10" workspacePath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/bike-icon.png">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="BikeStationEdge" sourceMapping="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikesAtStation']/@defaultLayer/@nodeMappings[name='BikeNode']" targetMapping="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikesAtStation']/@defaultLayer/@nodeMappings[name='StationNode']" targetFinderExpression="feature:currentStation">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="NodeCreation">
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="Station" nodeMappings="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikesAtStation']/@defaultLayer/@nodeMappings[name='BikeNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:CreateInstance" typeName="cb::Station" referenceName="stations"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:NodeCreationDescription" name="Bike" nodeMappings="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikesAtStation']/@defaultLayer/@nodeMappings[name='BikeNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:CreateInstance" typeName="cb::Bike" referenceName="bikes"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="EdgeCreation">
          <ownedTools xsi:type="tool_1:EdgeCreationDescription" name="BikeStationEdge" edgeMappings="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikesAtStation']/@defaultLayer/@edgeMappings[name='BikeStationEdge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:source">
                <subModelOperations xsi:type="tool:SetValue" featureName="currentStation" valueExpression="var:target"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="DiagramNavigation">
          <ownedTools xsi:type="tool_1:DoubleClickDescription" name="stationDoubleClick" mappings="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikesAtStation']/@defaultLayer/@nodeMappings[name='BikeNode']">
            <element name="element"/>
            <elementView name="elementView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Navigation" createIfNotExistent="true" diagramDescription="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikeInfomation']"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description_2:TreeDescription" name="StationOverview" domainClass="cb::City">
      <subItemMappings name="Station" domainClass="cb::Station" semanticCandidatesExpression="feature:eAllContents">
        <defaultStyle labelExpression="aql:self.id + ', ' + self.name + ', ' + self.address" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/station-icon-small.png">
          <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
        </defaultStyle>
        <subItemMappings name="Bike" domainClass="cb::Bike" semanticCandidatesExpression="feature:availableBikes">
          <defaultStyle labelExpression="aql:self.id + ', ' + self.name" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/bike-icon.png">
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </defaultStyle>
        </subItemMappings>
      </subItemMappings>
      <createTreeItem name="Station">
        <firstModelOperation xsi:type="tool:CreateInstance" typeName="cb::Station" referenceName="stations"/>
        <variables name="root" documentation="The semantic element of the tree."/>
        <variables name="element" documentation="The currently edited element."/>
        <variables name="container" documentation="The semantic element corresponding to the view container."/>
      </createTreeItem>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description_3:EditionTableDescription" name="Mechanics" domainClass="cb::City" initialHeaderColumnWidth="200">
      <ownedLineMappings name="Mechanic" domainClass="cb::Mechanic" headerLabelExpression="aql:self.id + ' ' + self.name"/>
      <ownedCreateLine name="Add Mechanic" mapping="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='Mechanics']/@ownedLineMappings[name='Mechanic']">
        <variables name="root" documentation="The semantic root element of the table."/>
        <variables name="element" documentation="The semantic currently edited element."/>
        <variables name="container" documentation="The semantic element corresponding to the view container."/>
        <firstModelOperation xsi:type="tool:CreateInstance" typeName="cb::Mechanic" referenceName="mechanics"/>
      </ownedCreateLine>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="BikeInfomation" titleExpression="aql:self.id + ', ' + self.name" domainClass="cb::Bike" enablePopupBars="true">
      <defaultLayer name="Default">
        <nodeMappings name="BikeInfoNode" semanticCandidatesExpression="var:self" domainClass="cb::Bike">
          <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql: 'ID: ' + self.id + '\nName: ' + self.name + '\nLast service: ' + self.lastServiceTime" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/bike-icon-small.png" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" width="30" height="10">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="ServiceReportInfoNode" semanticCandidatesExpression="aql:self.serviceReports" domainClass="cb::ServiceReport">
          <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:'Content: ' + self.content + '\nMechanic: ' + self.mechanic.id + ', ' + self.mechanic.name + '\nTime: ' + self.timestamp" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/file-alt-solid-blue-large.png" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" width="35" height="10">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="ServiceReportEdge" label="ServiceReportEdge" sourceMapping="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikeInfomation']/@defaultLayer/@nodeMappings[name='BikeInfoNode']" targetMapping="//@ownedViewpoints[name='AdminViewpoint']/@ownedRepresentations[name='BikeInfomation']/@defaultLayer/@nodeMappings[name='ServiceReportInfoNode']" targetFinderExpression="aql:self.serviceReports" sourceFinderExpression="aql:self" domainClass="cb::Bike" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12" showIcon="false">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="BikeInformationContainer" semanticCandidatesExpression="var:self" domainClass="cb::Bike" childrenPresentation="List">
          <subNodeMappings name="ServiceReportSubNode" semanticCandidatesExpression="feature:eAllContents" domainClass="cb::ServiceReport">
            <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:'Content: ' + self.content + '\nMechanic: ' + self.mechanic.id + ', ' + self.mechanic.name + '\nTime: ' + self.timestamp" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/file-alt-solid-blue-large.png" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            </style>
          </subNodeMappings>
          <style xsi:type="style:FlatContainerStyleDescription" borderSizeComputationExpression="1" labelSize="12" labelExpression="aql: self.id + ', ' + self.name + '\nStation: ' + self.currentStation.name + '\nLast service: ' + self.lastServiceTime" iconPath="/tdt4250.cb.model/tdt4250.cd.diagram/resources/bike-icon-small.png">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </containerMappings>
      </defaultLayer>
    </ownedRepresentations>
  </ownedViewpoints>
  <extensions xsi:type="properties:ViewExtensionDescription" name="CustomProperties">
    <categories name="Default Category">
      <pages name="Default Page" labelExpression="Page" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0"/>
      <groups name="ServiceReport" labelExpression="ServiceReport" domainClass="cb::ServiceReport" semanticCandidateExpression="var:self">
        <controls xsi:type="properties:SelectDescription" name="Mechanic" labelExpression="Mechanic" valueExpression="feature:mechanic" candidatesExpression="aql:self.eContainer().eContainer().mechanics" candidateDisplayExpression="aql:candidate.name">
          <initialOperation>
            <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool:SetValue" featureName="mechanic" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
        <controls xsi:type="properties:TextDescription" name="Content" labelExpression="Content" helpExpression="Write the report here.." valueExpression="feature:content">
          <initialOperation>
            <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool:SetValue" featureName="content" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
      </groups>
    </categories>
    <categories name="Category"/>
  </extensions>
</description:Group>
